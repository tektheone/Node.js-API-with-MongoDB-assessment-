{
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": ["error", "only-multiline"],
    "class-methods-use-this": "off",
    "no-underscore-dangle": "off"
  }
}
